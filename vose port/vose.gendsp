ffer small(\"small\");\r\nBuffer large(\"large\");\r\nBuffer posIndicesSmall(\"posIndicesSmall\");\r\nBuffer posIndicesLarge(\"posIndicesLarge\");\r\n\r\nn = dim(prob); \r\n\r\nfor(p = 0; p < n; p += 1) {\r\n\t\t\r\n\t\tpoke(small, -1, p);\r\n\t\t\r\n\t\tpoke(large, -1, p);\r\n}\r\n\r\naccumSmall = 0;\r\naccumLarge = 0;\r\n\r\nfor(p = 0; p < n; p += 1) {\r\n\t\t\r\n\t\tscaleProb = peek(prob, p);\t\r\n\t\t\t\t\t\r\n\t\t\tif( scaleProb < 1.) {\r\n\t\t\t\t\r\n\t\t\t\tpoke(small, p, accumSmall);\r\n\t\t\t\t\r\n\t\t\t\taccumSmall += 1;\r\n\t\t\r\n\t\t\t} else if( scaleProb >= 1. ) {\r\n\t\t\t\r\n\t\t\t\tpoke(large, p, accumLarge);\r\n\t\t\t\t\r\n\t\t\t\taccumLarge += 1;\r\n\t\t\r\n\t\t\t}\r\n}\t\t\t\r\n\r\nsmallEmpty = arrayEmpty(small, n);\r\nlargeEmpty = arrayEmpty(large, n);\r\ncomplete = smallEmpty && largeEmpty;\t\t\r\n\t\r\nwhile( complete < 1. ){\t\t\t\r\n\t\t\t\t\t\t\r\n\tl = pop(small, posIndicesSmall, n);\r\n\tg = pop(large, posIndicesLarge, n);\r\n\t\t\r\n\tpl = peek(prob, l);\r\n\r\n\tpoke(prob, pl, l);\r\n\t\r\n\tpoke(alias, g, l);\r\n\t\r\n\tpl2 = peek(prob, l);\r\n\t\r\n\tpg = peek(prob, g);\r\n\t\r\n\tpgNew = (pg + pl2) - 1;\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\r\n\tpoke(prob, pgNew, g);\r\n\t\r\n\tpg2 = peek(prob, g);\r\n\t\r\n\tif( pg2 < 1 ) {\r\n\t\t\r\n\t\tsbound = arrayBounds(small, n) + 1;\r\n\t\t\r\n\t\t//place new value at the last value in buffer that != -1.\r\n\t\tpoke(small, g, sbound);\r\n\t\r\n\t} else if ( pg2 >= 1 ) {\r\n\t\t\r\n\t\tlbound = arrayBounds(large, n) + 1;\r\n\t\t\r\n\t\tpoke(large, g, lbound);\r\n\t}\r\n\r\n}\r\n\r\nwhile( doneLarge != 1){\r\n\r\n\tg = pop(large, posIndicesLarge, n);\r\n\r\n\tpoke(prob, 1, g);\r\n\r\n\tdoneLarge = arrayEmpty(large, n);\r\n\r\n}\r\n\r\nwhile( doneSmall != 1){\r\n\r\n\tl = pop(small, posIndicesSmall, n);\r\n\r\n\tpoke(prob, 1, l);\r\n\r\n\tdoneSmall = arrayEmpty(small, n);\r\n\r\n}\r\n\r\ndice = int(scale(noise(), -1, 1, 0, n ));\r\n\r\ncoinProb = peek(prob, dice);\r\n\r\nerrorProb = peek(initProb, dice) > 0.;\r\n\r\ncoin = biasedCoin(coinProb);\r\n\r\naliasLookup = peek(alias, dice);\r\n\r\nchoose = coin == 0. ? dice : aliasLookup;\r\n\r\n//some probabilities that should be 1. seem to wrap to ~0.01; if -1 alias is returned return the input dice instead\r\n\r\nerrorCheck = choose == -1 ? dice : choose;\r\n\r\nresult = errorCheck;\r\n\r\nout4 = dice;\r\nout3 = coinProb;\r\nout2 = coin;\r\nout1 = result;\r\n",
